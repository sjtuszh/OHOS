{
  "app": {
    "bundleName": "com.szh.distributor",
    "vendor": "szh",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.szh.distributor",
    "name": ".MyApplication",
    "mainAbility": "com.szh.distributor.MainAbility",
    "deviceType": [
      "phone",
      "tablet"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "visible": true,
        "name": "com.szh.distributor.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "standard"
      },
      {
        "backgroundModes": [
          "dataTransfer",
          "bluetoothInteraction",
          "wifiInteraction",
          "multiDeviceConnection"
        ],
        "name": "com.szh.distributor.server",
        "icon": "$media:icon",
        "description": "$string:server_description",
        "type": "service"
      }
    ]
  }
}